		<logger level="INFO" doc:name="Logger" doc:id="5e6f353f-b99c-4801-ac77-662750290390" message="demo"/>
		<ee:transform doc:name="Transform Message" doc:id="deaaded7-181c-40a8-a355-8b481000237c" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
	"message":"Welcome to Demo Project",
	"code":200
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
</mule>
